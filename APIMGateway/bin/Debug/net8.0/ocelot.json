{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/accounting",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7110
        }
      ],
      "UpstreamPathTemplate": "/acc",
      "UpstreamHttpMethod": [ "Get" ]

    },
    {
      "DownstreamPathTemplate": "/api/inventory",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7242
        }
      ],
      "UpstreamPathTemplate": "/inv",
      "UpstreamHttpMethod": [ "Get" ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 5,
        "Limit": 3,
        "QuotaExceededMessage": "Too many requests, please try again later.",
        "HttpStatusCode": 429,
        "DisableRateLimitHeaders": false
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7193/"
  }
}
